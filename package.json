    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "open": "^9.1.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.22",
