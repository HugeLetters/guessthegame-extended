{
  "manifest_version": 3,
  "name": "guessthe.game extended",
  "description": "A few QoL improvement for the guessthe.game website",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png",
    "default_name": "guessthe.game extended"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://guessthe.game/", "https://guessthe.game/?fpg=*"],
      "js": ["./scripts/mainPage.js"],
      "css": ["./index.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [],
  "author": {
    "email": "ckuka71@gmail.com"
  }
}
